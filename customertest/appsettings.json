{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "customerone"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://127.0.0.1:8888/eureka",
      "shouldFetchRegistry": true,
      "healthCheckEnabled": true
    },
    "instance": {
      "port": 10001,
      "preferIpAddress": true,
      "ipAddress": "127.0.0.1"
    }
  }
}
